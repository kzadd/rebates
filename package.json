  },
  "keywords": [
    "frontend",
    "frontend-react",
    "ripley-rebates",
    "web-development"
  ],
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "name": "ripley-rebates",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "build:dev": "env-cmd -f .env.development tsc -b && vite build",
    "build:local": "env-cmd -f .env.local tsc -b && vite build",
    "build:prod": "env-cmd -f .env.production tsc -b && vite build",
    "build:staging": "env-cmd -f .env.staging tsc -b && vite build",
    "format": "prettier --write .",
    "format:lint": "npm run format && npm run lint",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:dev": "env-cmd -f .env.development vite",
    "start:local": "env-cmd -f .env.local vite",
    "start:prod": "env-cmd -f .env.production vite",
    "start:staging": "env-cmd -f .env.staging vite",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "npm run test --coverage",
    "test:e2e": "playwright test",
    "test:watch": "vitest --watch --passWithNoTests"
  },
  "type": "module",
  "version": "1.0.0"
}
